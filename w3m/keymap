# Ctrl  : C-, ^
# Escape: ESC-, M-, ^[
# Space : SPC, ' '
# Tab   : TAB, ^i, ^I
# Delete: DEL, ^?
# Up    : UP, ^[[A
# Down  : DOWN, ^[[B
# Right : RIGHT, ^[[C
# Left  : LEFT, ^[[D

keymap  U GOTO
keymap  J UP
keymap  K DOWN
keymap  R RELOAD
keymap  Z CENTER_H
keymap  z CENTER_V
keymap  b PREV_WORD
keymap  w NEXT_WORD
keymap  h MOVE_LEFT
keymap  l MOVE_RIGHT
keymap  g BEGIN
keymap  G END

keymap  < SHIFT_LEFT
keymap  > SHIFT_RIGHT
keymap  , LEFT
keymap  . RIGHT

keymap  C-@ MARK

keymap  C-a LINE_BEGIN
keymap  C-e LINE_END

keymap  m EXTERN
keymap  M EXTERN_LINK

keymap  SPC NEXT_PAGE
keymap  C-f NEXT_PAGE
keymap  C-b PREV_PAGE

keymap  C-[ LINK_BEGIN
keymap  C-] LINK_END
keymap  C-n NEXT_LINK
keymap  C-p PREV_LINK
keymap  C-m MOVE_LIST_MENU
keymap  C-l LIST_MENU
keymap  C-d SAVE_LINK
keymap  L LIST
keymap  D DOWNLOAD_LIST

keymap  M-a ADD_BOOKMARK
keymap  M-b BOOKMARK

keymap  M-g GOTO_LINE
keymap  M-k DEFINE_KEY
keymap  M-o SET_OPTION
keymap  M-c COMMAND
keymap  M-s SAVE
keymap  M-m MENU
keymap  M-n NEXT_MARK
keymap  M-p PREV_MARK
keymap  M-f LOAD
keymap  M-e PEEK
keymap  M-u PEEK_LINK

keymap  / SEARCH
keymap  ? SEARCH_BACK
keymap  n SEARCH_NEXT
neymap  p SEARCH_PREV

keymap  TAB-t NEW_TAB
keymap  TAB-j GOTO_LINK
keymap  TAB-u TAB_GOTO
keymap  TAB-k BACK
keymap  TAB-o TAB_LINK
keymap  TAB-c CLOSE_TAB
keymap  TAB-h PREV_TAB
keymap  TAB-l NEXT_TAB
keymap  TAB-m TAB_MENU

keymap  C-r REDRAW
keymap  C-z SUSPEND
keymap  C-h HISTORY
keymap  H HELP
keymap  O OPTIONS
keymap  = INFO
keymap  v VIEW

keymap  ! SHELL
keymap  \"  REG_MARK
keymap  # PIPE_SHELL
keymap  : MARK_URL
keymap  ";" MARK_WORD
keymap  @ READ_SHELL
keymap  | PIPE_BUF
