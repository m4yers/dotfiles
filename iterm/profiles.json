    "Normal Font": "Menlo-Regular 12",
    "ASCII Anti Aliased": true,
    "Use Non-ASCII Font": false,
    "Use Italic Font": true,
    "Use Bright Bold": true,
    "Use Bold Font": false,

    "Flashing Bell": false,
    "Silence Bell": false,
    "Visual Bell": true
  }]
}
